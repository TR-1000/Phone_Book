<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <% include ./partials/head.ejs %>
  </head>
  <body>
    <div class="container">
      <img src="<%=phone.img%>" alt="">
      <h1><%=phone.name%></h1>
      <h2><%=phone.model%></h2>
      Current Status: <%=phone.status%>
      <article class="">
        <%=phone.description%>
      </article>
      <article class="">
        <%=phone.log%>
      </article>

      <div class="gallery">

        <% for (image of phone.gallery.split(",")) { %>
          <img src="<%=image %>" alt="">
        <% } %>

      </div>

      <div class="album">
        <% for (image of phone.album.split(",")) { %>
          <img src="<%=image %>" alt="">
        <% } %>
      </div>

    </div>
  </body>
</html>
